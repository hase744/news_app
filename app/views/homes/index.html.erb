<h1 class="news_title"><%=DateTime.now.to_date%> エンジニア関連ニュース</h1>
<% @videos.each do |video| %>
    <div><iframe width="560" height="315" src="https://www.youtube.com/embed/<%= video["youtube_id"] %>" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe></div>
    <div><%=video["title"]%></div>
<% end %>
